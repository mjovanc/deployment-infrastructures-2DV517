upstream backend {
        ip_hash;
        server {{ hostvars['wordpress']['ansible_host'] }}:80 max_fails=3 fail_timeout=30s;
}

server {
        listen 80;
        listen [::]:80 ipv6only=on;

        server_name cscloud6-254.lnu.se;

        location / {
                proxy_pass http://backend;
        }
}

